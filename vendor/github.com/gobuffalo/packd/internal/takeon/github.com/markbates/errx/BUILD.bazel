load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "errx.go",
        "version.go",
    ],
    importmap = "valerian/vendor/github.com/gobuffalo/packd/internal/takeon/github.com/markbates/errx",
    importpath = "github.com/gobuffalo/packd/internal/takeon/github.com/markbates/errx",
    visibility = ["//vendor/github.com/gobuffalo/packd:__subpackages__"],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
